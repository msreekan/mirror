<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="shugaoye"
           fetch="." />

  <remote  name="github"
           fetch="https://github.com" />

  <default revision="master"
           remote="github"
           sync-j="4" />

  <!-- Local repositories at github/shugaoye -->
  <project name="docker-alpine-git" remote="shugaoye" />

  <!-- GitHub repositories -->
  <project name="github/gitignore"/>

</manifest>